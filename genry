<?php
require_once '.genry/autoload.php';

use Splot\Framework\Framework;
use MD\Genry\Application as GenryApplication;

class Application extends GenryApplication
{

    public function loadModules() {
        $modules = parent::loadModules();
        
        // add your own custom modules to $modules array

        return $modules;
    }

}

Framework::run(new Application(), 'dev', true, Framework::MODE_CONSOLE);