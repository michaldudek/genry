{
    "name": "michaldudek/genry",
    "description": "Static page generator that uses Twig.",
    "keywords": ["generator", "static", "twig", "html"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "require": {
        "php": ">=5.5",
        "michaldudek/foundation": ">=0.9,<2.0",
        "splot/framework": ">=0.9,<2.0",
        "splot/assets-module": ">=0.9,<2.0",
        "splot/twig-module": ">=0.9,<2.0",
        "michelf/php-markdown": ">=1.3,<2.0",
        "symfony/yaml": ">=2.1,<3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd" : "~2.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Genry\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "bin": [
        "bin/genry"
    ]
}
